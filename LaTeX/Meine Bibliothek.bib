	shortjournal = {Transportation Research Part A: Policy and Practice},
	author = {Zhang, Jie and Wang, David Z. W. and Meng, Meng},
	urldate = {2025-07-22},
	date = {2018-12-01},
	keywords = {On-demand public bus, Park and ride, Public transport, Service selection, Social welfare},
	file = {Eingereichte Version:/Users/alexanderklaus/Zotero/storage/2WY28THD/Zhang et al. - 2018 - Which service is better on a linear travel corridor Park & ride or on-demand public bus.pdf:application/pdf;ScienceDirect Snapshot:/Users/alexanderklaus/Zotero/storage/BQFT8C4N/S096585641731652X.html:text/html},
