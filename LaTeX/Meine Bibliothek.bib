	urldate = {2025-07-22},
	date = {2018},
	langid = {english},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/itor.12476},
	keywords = {conventional public transportation, demand-responsive transit systems, on-demand transportation, simulation},
	file = {Full Text PDF:/Users/alexanderklaus/Zotero/storage/AQNWY9FZ/Archetti et al. - 2018 - A simulation study of an on-demand transportation system.pdf:application/pdf},
