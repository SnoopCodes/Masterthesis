      \strng{namehash}{d28ce98879459e12a80952cbe14d6f74}
      \strng{fullhash}{4d840a66de711505e5f58a5ab310286d}
      \strng{fullhashraw}{4d840a66de711505e5f58a5ab310286d}
      \strng{bibnamehash}{4d840a66de711505e5f58a5ab310286d}
      \strng{authorbibnamehash}{4d840a66de711505e5f58a5ab310286d}
      \strng{authornamehash}{d28ce98879459e12a80952cbe14d6f74}
