    \entry{vosooghi_shared_2019}{article}{}{}
      \name{author}{4}{}{%
        {{hash=6e03e43761cde6c3b942743c8cfb1fe6}{%
           family={Vosooghi},
           familyi={V\bibinitperiod},
           given={Reza},
