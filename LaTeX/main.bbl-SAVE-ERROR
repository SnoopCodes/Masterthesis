      \strng{namehash}{5ab7681895168cc67f62b1432bf7755d}
      \strng{fullhash}{1b37dc4c73e26a1f847dafbfb27ab581}
      \strng{fullhashraw}{1b37dc4c73e26a1f847dafbfb27ab581}
      \strng{bibnamehash}{1b37dc4c73e26a1f847dafbfb27ab581}
