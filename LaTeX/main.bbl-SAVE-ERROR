      \strng{bibnamehash}{d91b2b041996a72adb3f27efde3cdfb0}
      \strng{authorbibnamehash}{d91b2b041996a72adb3f27efde3cdfb0}
      \strng{authornamehash}{518e29bd4c0f01b6bf67775020b6af6a}
      \strng{authorfullhash}{d91b2b041996a72adb3f27efde3cdfb0}
