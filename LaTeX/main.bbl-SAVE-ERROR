      \field{shortjournal}{Public Transp}
      \field{title}{Analytical models for comparing operational costs of regular bus and semi-flexible transit services}
      \field{urlday}{9}
      \field{urlmonth}{8}
      \field{urlyear}{2025}
      \field{volume}{12}
      \field{year}{2020}
      \field{dateera}{ce}
      \field{urldateera}{ce}
      \field{pages}{147\bibrangedash 169}
