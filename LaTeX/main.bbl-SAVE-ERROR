      \strng{namehash}{77f2e4abe4de2f9e509deed3da0f2318}
      \strng{fullhash}{77f2e4abe4de2f9e509deed3da0f2318}
      \strng{fullhashraw}{77f2e4abe4de2f9e509deed3da0f2318}
      \strng{bibnamehash}{77f2e4abe4de2f9e509deed3da0f2318}
