      \strng{namehash}{7e03851d01f64ec9b6c1cd8e326f43b0}
      \strng{fullhash}{2a37bd802fccd18f5d75a605ba65bdcf}
      \strng{fullhashraw}{2a37bd802fccd18f5d75a605ba65bdcf}
      \strng{bibnamehash}{2a37bd802fccd18f5d75a605ba65bdcf}
      \strng{authorbibnamehash}{2a37bd802fccd18f5d75a605ba65bdcf}
      \strng{authornamehash}{7e03851d01f64ec9b6c1cd8e326f43b0}
