    \entry{zwick_analysis_2020}{article}{}{}
      \name{author}{2}{}{%
        {{hash=b7712c5ab5a14586cd46058a5bdda1b4}{%
           family={Zwick},
           familyi={Z\bibinitperiod},
           given={Felix},
