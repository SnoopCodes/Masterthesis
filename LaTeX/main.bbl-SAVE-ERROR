      \strng{namehash}{73a9f13a2da511955b5827f3bb662c81}
      \strng{fullhash}{73a9f13a2da511955b5827f3bb662c81}
      \strng{fullhashraw}{73a9f13a2da511955b5827f3bb662c81}
      \strng{bibnamehash}{73a9f13a2da511955b5827f3bb662c81}
      \strng{authorbibnamehash}{73a9f13a2da511955b5827f3bb662c81}
