    \entry{wang_multilevel_2014}{article}{}{}
      \name{author}{5}{}{%
        {{hash=3ad93e4c675a68a22a9c2c52b38e2751}{%
           family={Wang},
           familyi={W\bibinitperiod},
           given={Chao},
