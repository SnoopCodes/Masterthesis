      \strng{namehash}{2ed45a9cf2d5e03ba2c355efef033003}
      \strng{fullhash}{d2fdfc368466e605f3fc522e3ed756b5}
      \strng{fullhashraw}{d2fdfc368466e605f3fc522e3ed756b5}
      \strng{bibnamehash}{d2fdfc368466e605f3fc522e3ed756b5}
      \strng{authorbibnamehash}{d2fdfc368466e605f3fc522e3ed756b5}
      \strng{authornamehash}{2ed45a9cf2d5e03ba2c355efef033003}
      \strng{authorfullhash}{d2fdfc368466e605f3fc522e3ed756b5}
      \strng{authorfullhashraw}{d2fdfc368466e605f3fc522e3ed756b5}
