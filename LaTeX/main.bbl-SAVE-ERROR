      \strng{namehash}{5870beeebb7bcef335b892cb0b8b95f7}
      \strng{fullhash}{b91deb4992be132244716847ffe908f8}
      \strng{fullhashraw}{b91deb4992be132244716847ffe908f8}
      \strng{bibnamehash}{b91deb4992be132244716847ffe908f8}
