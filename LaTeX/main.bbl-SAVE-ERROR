    \entry{engelhardt_speed-up_2020}{misc}{}{}
      \name{author}{3}{ul=2}{%
        {{hash=68e5ea7dce7ed9f82506c1e0a6796d98}{%
           family={Engelhardt},
           familyi={E\bibinitperiod},
           given={Roman},
