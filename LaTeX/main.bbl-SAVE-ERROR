      \strng{namehash}{e9d16361503d44c7d4c8e5d84c89b0e5}
      \strng{fullhash}{0d40a35fa0fae26d9b366087cf175d54}
      \strng{fullhashraw}{0d40a35fa0fae26d9b366087cf175d54}
      \strng{bibnamehash}{0d40a35fa0fae26d9b366087cf175d54}
      \strng{authorbibnamehash}{0d40a35fa0fae26d9b366087cf175d54}
      \strng{authornamehash}{e9d16361503d44c7d4c8e5d84c89b0e5}
      \strng{authorfullhash}{0d40a35fa0fae26d9b366087cf175d54}
