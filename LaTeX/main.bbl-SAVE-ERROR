    \entry{takamatsu_bus_2020}{article}{}{}
      \name{author}{2}{}{%
        {{hash=38df8d8744319ea55de7103993eee4d3}{%
           family={Takamatsu},
           familyi={T\bibinitperiod},
           given={Mizuyo},
           giveni={M\bibinitperiod}}}%
        {{hash=54896b7425b918a55ed360a66845bfa2}{%
           family={Taguchi},
           familyi={T\bibinitperiod},
           given={Azuma},
           giveni={A\bibinitperiod}}}%
