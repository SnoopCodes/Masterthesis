    \entry{alonso-gonzalez_what_2021}{article}{}{}
      \name{author}{5}{ul=2}{%
        {{hash=f5c3c2e796802e5eaaeadf65a6a72851}{%
           family={Alonso-González},
           familyi={A\bibinithyphendelim G\bibinitperiod},
           given={María\bibnamedelima J.},
